            ],
            "tsConfig": "projects/daily-admin/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/daily-admin/public"
              }
            ],
            "styles": [
              "projects/daily-admin/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "85e00dc1-91eb-4dbf-a45c-935eebe3887a"
  }
}
